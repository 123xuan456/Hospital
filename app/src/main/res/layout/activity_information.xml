<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F0EFF5"
    tools:context="d.hospital.activity.InformationActivity">
    <include layout="@layout/layou_finish"
        android:id="@+id/include4"></include>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="75dp"
        android:layout_below="@+id/include4"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:background="@drawable/biankuang_bottom"
        android:layout_alignParentStart="true"
        android:id="@+id/relativeLayout4">


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="头像"
            android:textSize="15sp"
            android:gravity="center"
            android:id="@+id/textView13"
            android:layout_alignParentStart="true"
            android:layout_marginStart="15dp"
            android:layout_alignTop="@+id/view"
            android:layout_alignBottom="@+id/view" />


        <de.hdodenhof.circleimageview.CircleImageView
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:src="@mipmap/c"
            android:id="@+id/view"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_centerVertical="true"
            android:layout_toStartOf="@+id/imageView" />


        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView"
            android:src="@mipmap/hui"
            android:paddingEnd="10dp"
            android:paddingLeft="10dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/biankuang_bottom"
        android:layout_below="@+id/relativeLayout4"
        android:layout_alignParentStart="true"
        android:id="@+id/relativeLayout5">


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="昵称"
            android:textSize="15sp"
            android:id="@+id/textView14"
            android:layout_marginStart="15dp"
            android:layout_centerVertical="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:id="@+id/name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="野区哈士奇"
            android:textSize="15sp"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true"
            android:layout_toLeftOf="@+id/imageView1"
            android:layout_toRightOf="@+id/textView14"
            android:gravity="right|center"
            android:paddingRight="10dp"
            />


        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView1"
            android:src="@mipmap/hui"
            android:paddingEnd="10dp"
            android:paddingLeft="10dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#ffffff"
        android:layout_below="@+id/relativeLayout5"
        android:layout_alignParentStart="true"
        android:id="@+id/relativeLayout6">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="性别"
            android:textSize="15sp"
            android:id="@+id/textView1"
            android:layout_marginStart="15dp"
            android:layout_centerVertical="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:id="@+id/sex"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="男"
            android:textSize="15sp"
            android:paddingRight="10dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true"
            android:layout_toLeftOf="@+id/imageView2"
            android:layout_toRightOf="@+id/textView1"
            android:gravity="right|center"
            />


        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView2"
            android:src="@mipmap/hui"
            android:paddingEnd="10dp"
            android:paddingLeft="10dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true" />



    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_below="@+id/relativeLayout6"
        android:background="@drawable/biankuang_bottom"
        android:layout_marginTop="15dp"
        android:layout_alignParentStart="true"
        android:id="@+id/relativeLayout7">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/shouji"
            android:padding="10dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentTop="true"
            android:id="@+id/imageView10" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="手机号"
            android:textSize="15sp"
            android:id="@+id/textView11"
            android:layout_centerVertical="true"
            android:layout_toEndOf="@+id/imageView10" />



        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView3"
            android:src="@mipmap/hui"
            android:paddingEnd="10dp"
            android:paddingLeft="10dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true" />

        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="未绑定"
            android:id="@+id/textView15"
            android:layout_centerVertical="true"
            android:layout_toStartOf="@+id/imageView3" />


    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/biankuang_bottom"
        android:layout_below="@+id/relativeLayout7"
        android:layout_alignParentStart="true"
        android:id="@+id/relativeLayout8">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/weixin"
            android:padding="10dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentTop="true"
            android:id="@+id/imageView14" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView15"
            android:src="@mipmap/hui"
            android:paddingEnd="10dp"
            android:paddingLeft="10dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="微信账号"
            android:textSize="15sp"
            android:id="@+id/textView16"
            android:layout_centerVertical="true"
            android:layout_toEndOf="@+id/imageView14" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="未绑定"
            android:id="@+id/textView17"
            android:layout_centerVertical="true"
            android:layout_toStartOf="@+id/imageView15" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#ffffff"
        android:layout_below="@+id/relativeLayout8"
        android:layout_alignParentStart="true"
        android:id="@+id/relativeLayout9">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/qq"
            android:padding="10dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentTop="true"
            android:id="@+id/imageView16" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView17"
            android:src="@mipmap/hui"
            android:paddingEnd="10dp"
            android:paddingLeft="10dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="QQ号"
            android:textSize="15sp"
            android:id="@+id/textView18"
            android:layout_centerVertical="true"
            android:layout_toEndOf="@+id/imageView16" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="已绑定"
            android:id="@+id/textView19"
            android:layout_centerVertical="true"
            android:layout_toStartOf="@+id/imageView17" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout10"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="15dp"
        android:background="#ffffff"
        android:layout_below="@+id/relativeLayout9"
        android:layout_alignParentStart="true">



        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView19"
            android:src="@mipmap/hui"
            android:paddingEnd="10dp"
            android:paddingLeft="10dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="我的地址"
            android:textSize="15sp"
            android:id="@+id/textView20"
            android:layout_marginStart="15dp"
            android:layout_centerVertical="true"
             />

    </RelativeLayout>


</RelativeLayout>
